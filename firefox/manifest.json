{
  "manifest_version": 3,
  "name": "Job Link Extractor",
  "version": "1.0",
  "description": "Extracts and cleans job links from specified websites.",
  "permissions": ["activeTab", "scripting", "clipboardWrite", "storage"],
  "host_permissions": ["*://*.seek.co.nz/*", "*://*.seek.com.au/*", "*://*.linkedin.com/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "job-link-extractor@example.com",
      "strict_min_version": "109.0"
    }
  }
}
